<script type="text/javascript">


function displayDate() {
  var user = document.getElementById("text_user");
  var pass = document.getElementById("text_pass");

if(user.value=="123" && pass.value=="123" ){
  alert('密码正确！');
}else{
  pass.value="";
  alert('密码错误!');
}


}
</script>
<body>
   <p>账号：</p>
   <input type='text' id='text_user' value=""/>
   <p>密码:</p>
   <input type='password' id='text_pass' value=""/>
   <button onclick="displayDate()">确定</button>

</body>
